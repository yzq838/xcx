.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 字体图标 */
@font-face {
  font-family: 'iconfont';  /* project id 1737571 */
  src: url('//at.alicdn.com/t/font_1737571_qobjk8x8pt.eot');
